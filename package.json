{
  "name": "magin-concept",
  "private": true,
  "version": "0.1.0",
  "homepage": "https://magin.blue",
  "description": "magin - Learn to watch a novel like a Pixar director",
  "author": "rig√©lblu (https://www.rigelblu.com)",
  "license": "MIT",
  "scripts": {
    "clean": "yarn docker:clean && yarn next:clean",
    "build:ssg": "yarn next:build:ssg",
    "start": "node server.js",
    "start:next": "source .env .env.development && next dev -p 3000",
    "start:live": "next start -p 8080",
    "test": "jest --passWithNoTests",
    "test:update-ss": "jest --updateSnapshot",
    "lint-staged": "lint-staged",
    "lint": "yarn lint:next",
    "lint:eslint": "eslint --cache --max-warnings=0",
    "lint:next": "next lint",
    "lint:style": "stylelint --allow-empty-input --cache --max-warnings=0",
    "next:clean": "rimraf .next build",
    "next:build": "next build",
    "next:build:ssg": "yarn next:build && next export -o build",
    "prettier": "prettier --write --ignore-unknown",
    "prettier:check": "prettier --check --ignore-unknown",
    "docker:clean": "rimraf docker",
    "docker:build": "docker build -t mgn-app docker --target runner-ssg",
    "docker:build:prod": "docker build -t mgn-app docker --target runner-ssg",
    "docker:build-run": "yarn docker:build && yarn docker:run",
    "docker:run": "docker run -d -p 8080:8080 mgn-app",
    "docker:stage-context": "mkdir docker && cp -rf Dockerfile additional.d.ts jest.config.js next-env.d.ts next.config.js package.json public server.js src tsconfig*.json tsconfigs yarn.lock docker",
    "docker:scan": "docker scan mgn-app",
    "docker:rebuild": "yarn docker:clean && yarn docker:stage-context && yarn docker:build-run"
  },
  "keywords": [
    "magin"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:rigelblu/magin-concept.git"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@next/eslint-plugin-next": "^13.0.2",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.2",
    "@types/react": "^18.0.2",
    "@types/react-dom": "^18.0.2",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.4.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.3",
    "next": "^13.0.2",
    "postcss": "^8.4.13",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "primeflex": "^3.1.3",
    "primeicons": "^6.0.1",
    "primereact": "^8.7.1",
    "react-test-renderer": "^18.0.2",
    "react-transition-group": "^4.4.2",
    "react": "^18.0.2",
    "react-dom": "^18.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.50.0",
    "stylelint": "^14.5.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-csstree-validator": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
